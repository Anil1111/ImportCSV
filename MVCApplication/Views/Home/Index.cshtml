@model MVCApplication.ViewModels.IndexViewModel

<h1>Categories</h1>
<h2>View All</h2>
<ul>
    @foreach (KeyValuePair<string, string> column in Model.columns)
    {
        <li>
            <a asp-controller="List" asp-action="Values" asp-route-column="@column.Key">@column.Value</a>
        </li>
    }
</ul>

